{
  "manifest_version": 2,
  "name": "NoScript",
  "version": "10.1.0",
  "description": "NoScript Security Suite",

  "permissions": [
      "contextMenus",
      "privacy",
      "storage",
      "tabs",
      "webNavigation",
      "webRequest",
      "webRequestBlocking",
      "<all_urls>"
  ],

  "background": {
    "page": "background.html"
  }
}
